#include <bits/stdc++.h>
using namespace std;

int main() 
{
  int n; cin >> n;
   vector<int>v(n);
   for(int i=0;i<n;i++){
     cin >> v[i];
   }
   
   vector<int>suffix(n);
   suffix[n-1]=v[n-1];
   for(int i=n-1;i>=0;i--){
     suffix[i]=suffix[i+1]+v[i];
   }
  // for(auto i : suffix){
  //   cout << i << " ";
  // 
    for(int i=n-1;i>=0;i--){
      cout << suffix[i] << " ";
    }
  
  
    return 0;
}
