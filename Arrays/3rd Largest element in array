#include <iostream>
using namespace std;

int main() 
{
    int n;
    cin >> n;
    
    int arr[n];
    for(int i=0;i<n;i++){
      cin >> arr[i];
    }
    
    int first=-1,second=-1;
    for(int i=0;i<n;i++){
      if(arr[i]>first){
        first=arr[i];
      }
    }
    for(int i=0;i<n;i++){
      if(arr[i]!=first && arr[i]>second){
        second = arr[i];
      }
    }
    int third=-1;
    for(int i=0;i<n;i++){
      if(arr[i]!=first && arr[i]!=second && arr[i]>third){
        third=arr[i];
      }
    }
    cout << third;
    return 0;
}
