#include <iostream>
using namespace std;

int main() 
{
   int n;
   cin >> n;
   
   int arr[n];
   for(int i=0;i<n;i++){
     cin >> arr[i];
   }
   
   int key ;
   cin >> key;
   
  int ans=-1;
  int start=0;
  int end=n-1;
  int mid;
  
  while(start<=end){
    mid=start+(end-start)/2;
    if(arr[mid]==key){
    ans=mid;
    break;
  }
    else if(arr[mid]<key){
    end =mid-1;
    }else{
    start=mid+1;
    }
  }
  cout << ans;
    return 0;
}
