#include <iostream>
using namespace std;

int main() 
{
   int n=5;
   int arr[5]={1,2,3,4,5};
   
   for(int i=0;i<n-1;i++){
     bool flag=false;
     for(int j=0;j<=n-i-2;j++){
       if(arr[j]>arr[j+1]){
         swap(arr[j],arr[j+1]);
         flag=true;
       }
       if(flag==0){
         break;
       }
     }
   }
   
   for(int i=0;i<n;i++){
     cout << arr[i] << " ";
   }
    return 0;
}
